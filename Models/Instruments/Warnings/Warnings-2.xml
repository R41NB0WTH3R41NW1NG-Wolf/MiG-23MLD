<?xml version="1.0" encoding="UTF-8"?>
<!-- Labels based on MiG-29 version -->
<PropertyList>
    <path>warnings1.ac</path>
    <model>
        <path>warnings.ac</path>
    </model>
    <effect>
        <inherits-from>Aircraft/MiG-23MLD/Models/Effects/instruments/lmWarnings</inherits-from>
        <object-name>warnings</object-name>
    </effect>
    <animation>
        <object-name>warnings</object-name>
        <type>select</type>
        <condition>
            <greater-than>
                <property>fdm/jsbsim/electric/sources/output-dc</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>
    <animation>
        <object-name>L06</object-name>
        <type>select</type>
        <condition>
            <property>fdm/jsbsim/warning-lights/L600</property>
        </condition>
    </animation>
    <animation>
        <object-name>L07</object-name>
        <type>select</type>
        <condition>
            <and>
                <greater-than>
                    <property>orientation/alpha-deg</property>
                    <value>15</value>
                </greater-than>
                <greater-than>
                    <property>fdm/jsbsim/fcs/wing-sweep</property>
                    <value>0</value>
                </greater-than>
            </and>
        </condition>
    </animation>
    <animation>
        <object-name>L08</object-name>
        <type>select</type>
        <condition>
            <less-than>
                <property>fdm/jsbsim/electric/sources/dc-gen</property>
                <value>18</value>
            </less-than>
        </condition>
    </animation>
    <animation>
        <object-name>L01</object-name>
        <object-name>L02</object-name>
        <object-name>L03</object-name>
        <object-name>L04</object-name>
        <object-name>L05</object-name>
        <object-name>L09</object-name>
        <object-name>L10</object-name>
        <type>select</type>
        <condition>
            <greater-than>
                <value>0</value>
                <value>1</value>
            </greater-than>
        </condition>
    </animation>
</PropertyList>
